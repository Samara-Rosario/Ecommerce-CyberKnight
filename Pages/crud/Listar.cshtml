@page
@model Ecommerce_CyberKnight.Pages.crud.ListarModel
@{
}


<table>
    <thead>
        <tr>
            <td>Id</td>
            <td>Cpf</td>
            <td>Nome</td>
            <td>Email</td>
            <td>Telefone</td>
            <td>Cep</td>
            <td>Login</td>
            <td>Senha</td>
            <td>Endereço</td>
            <td>Ações</td>
        </tr>
    </thead>
    <tbody>@foreach(var cliente in Model.Clientes) {
        <tr>
            <td>@cliente.Id</td>
            <td>@cliente.Cpf</td>
            <td>@cliente.Nome</td>
            <td>@cliente.Email</td>
            <td>@cliente.Telefone</td>
            <td>@cliente.Cep</td>
            <td>@cliente.Login</td>
            <td>@cliente.Senha</td>
            <td>@cliente.Endereco</td>
            <td>
                <a>Editar</a>
                <form
                    method="post"
                    asp-page-handler="delete"
                    asp-route-id="@cliente.Id"
                >
                    <button>Excluir</button>
                </form>
            </td>
        </tr>
    }</tbody>
</table>